{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "noImplicitReturns": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "target": "es5",
        "experimentalDecorators": true,
        "outDir": "./files"
    },
    "include": [
        "typescript/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
    
}